{
  "name": "knot-vscode-plugin",
  "displayName": "Knot Language",
  "description": "Knot language support.",
  "categories": [
    "Programming Languages"
  ],
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.30.2"
  },
  "private": true,
  "repository": "https://github.com/effervescentia/knot.git",
  "homepage": "https://github.com/effervescentia/knot/blob/master/extensions/vscode/README.md",
  "contributes": {
    "languages": [
      {
        "id": "knot",
        "aliases": [
          "Knot",
          "knot"
        ],
        "extensions": [
          ".kn",
          ".knot"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "knot",
        "scopeName": "source.knot",
        "path": "./syntaxes/knot.tmLanguage"
      }
    ]
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#31717b",
    "theme": "dark"
  },
  "publisher": "knot-lang"
}
