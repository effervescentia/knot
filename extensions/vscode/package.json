{
  "name": "knot-language",
  "displayName": "Knot Language",
  "description": "Knot language support.",
  "version": "0.0.1",
  "publisher": "effervescentia",
  "engines": {
    "vscode": "^1.30.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "knot",
      "aliases": ["Knot", "knot"],
      "extensions": [
        ".kn",
        ".knot"
      ],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "knot",
      "scopeName": "source.knot",
      "path": "./syntaxes/knot.tmLanguage"
    }]
  }
}