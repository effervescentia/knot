{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "yarn",
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "files.associations": {
    ".fixpackrc": "json",
    ".czrc": "json",
    ".nyrc": "json",
    ".npsrc": "json",
    ".huskyrc": "json",
    ".lintstagedrc": "json"
  }
}
