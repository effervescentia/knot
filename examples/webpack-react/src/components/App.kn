import Counter from ".components.Counter";

view Greeting(name: string) -> "Hello, " + name + "!";

main view App(name: string) ->
  <div>
    // <Greeting name />
    <Counter />
  </div>;

state SimpleState {
  value: string = "abc";
  other = $value;

  mut setValue() -> {
    $value = "def";
  }
}

view OtherView ~ SimpleState -> {
  <div>{$value + "def"}<button onClick={$setValue}>{"wooooah"}</button></div>
}
