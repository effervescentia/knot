const rootStyle = style {
  height: $px(200.0),
  backgroundColor: $navy,
};

const topStyle = style {
  height: $px(100.0),
  width: $px(200.0),
  backgroundColor: $pink,
};

const bottomStyle = style {
  height: $px(175.0),
  width: $px(150.0),
  backgroundColor: $violet,
};

main view Stylish ->
  <div::rootStyle id="stylish">
    <div::topStyle id="stylish__child--top" />
    <div::bottomStyle id="stylish__child--bottom" />
  </div>
;
