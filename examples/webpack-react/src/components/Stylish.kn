style FancyStyle -> {
  .root {
    height: $px(200.0);
    backgroundColor: $navy;
  }

  .child {
    height: $px(100.0);
    width: $px(200.0);
  }

  .lhs {
    backgroundColor: $pink;
  }
  .rhs {
    backgroundColor: $violet;
  }
}

main view StyledView ~ FancyStyle ->
  <div .root id="stylish">
    <div .child .lhs id="stylish__child--top" />
    <div .child .rhs id="stylish__child--bottom" />
  </div>
;
