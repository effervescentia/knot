const rootStyle = style {
  height: $px(200.0),
  backgroundColor: $navy,
};

const Container = div::rootStyle;

const TopComponent = div::{
  height: $px(100.0),
  width: $px(200.0),
  backgroundColor: "#ec6810",
};

const bottomStyle = style {
  height: $px(175.0),
  width: $px(150.0),
  backgroundColor: $violet,
};

main view Stylish ->
  <Container id="stylish">
    <TopComponent id="stylish__child--top" />
    <div::bottomStyle id="stylish__child--bottom" />
  </Container>
;
